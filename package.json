{
    "name": "mpl",
    "version": "0.0.1",
    "description": "A compiler for a Mutable Programming Langauge",
    "repository": "https://github.com/drew-gross/mpl",
    "license": "UNLICENSED",
    "main": "built/index.js",
    "ava": {
        "files": [
            "built/test.js"
        ]
    },
    "scripts": {
        "benchmark": "rimraf built && tsc && node ./built/benchmark.js",
        "test": "rimraf built && tsc && ava --verbose && tslint --project ./package.json  ./**/*.ts && npm run prettier-check",
        "test-fast": "tsc && ava --verbose",
        "test-all": "npm run test && npm run benchmark",
        "test-watch": "parallelshell 'ava --watch --no-color' 'tsc --watch'",
        "tsc": "tsc",
        "tsc-watch": "tsc --watch",
        "build+install": "tsc && npm link",
        "test-debug": "tsc && node --inspect --inspect-brk node_modules/ava/profile.js built/test.js",
        "debug-test-case": "tsc && node ./built/debug-test-case.js",
        "debug-test-case-gen": "tsc && node --inspect --inspect-brk ./built/debug-test-case.js",
        "profile-test-case-gen": "tsc && node_modules/.bin/0x ./built/debug-test-case.js",
        "cloc": "cloc --exclude-dir=node_modules,built --not-match-f='npm-shrinkwrap.json|package.json|tsconfig.json|prettier-config.json|test.ts|test-utils.ts' .",
        "cloc-all": "cloc --exclude-dir=node_modules,built . --not-match-f='npm-shrinkwrap.json'",
        "prettier-check": "prettier --config package.json --list-different '**/*.ts'"
    },
    "bin": {
        "dump-builtins-to-cfg-svg": "built/bin/dump-builtins-to-cfg-svg.js"
    },
    "author": "Drew Gross",
    "dependencies": {
        "0x": "4.8.2",
        "@types/node": "12.7.4",
        "ava": "2.3.0",
        "chalk": "2.4.2",
        "child-process-promise": "2.2.1",
        "clone": "2.1.2",
        "commander": "3.0.1",
        "deepcopy": "2.0.0",
        "fs-extra": "8.1.0",
        "graphlib": "2.1.7",
        "graphlib-dot": "0.6.2",
        "inquirer": "7.0.0",
        "lodash": "4.17.15",
        "node": "12.8.1",
        "omit-deep": "0.3.0",
        "parallelshell": "3.0.2",
        "prettier": "1.18.2",
        "rimraf": "3.0.0",
        "tmp-promise": "2.0.2",
        "tslint": "5.19.0",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.5.3"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "devDependencies": {
        "shuffle-seed": "^1.1.6"
    }
}
